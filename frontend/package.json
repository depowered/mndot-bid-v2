        "sveltekit",
        "flowbite",
        "flowbite-svelte",
        "tailwindcss"
    ],
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/depowered/mndot-bid-v2.git"
    },
    "dependencies": {
        "@duckdb/duckdb-wasm": "^1.27.0",
