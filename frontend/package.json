{
    "name": "mndot-bid-frontend",
    "version": "0.1.0",
    "description": "SvelteKit dashboard powered by duckdb-WASM, Observable Plot, and Flowbite-Svelte components",
    "author": {
        "name": "Devin Power",
        "email": "dpwr16@gmail.com",
        "url": "https://mndotbidprices.com"
    },
    "bugs": "https://github.com/depowered/mndot-bid-v2/issues",
    "homepage": "https://github.com/depowered/mndot-bid-v2",
    "license": "MIT",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@playwright/test": "^1.30.0",
        "@sveltejs/adapter-auto": "2.0.0",
        "@sveltejs/adapter-cloudflare": "^2.3.0",
        "@sveltejs/kit": "^1.20.4",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "flowbite": "^1.6.6",
        "flowbite-svelte": "^0.38.5",
        "flowbite-svelte-blocks": "^0.1.6",
        "postcss": "^8.4.23",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0",
        "svelte": "^3.55.1",
        "svelte-check": "^2.10.3",
        "svelte-heros": "^2.6.0",
        "svelte-preprocess": "^5.0.3",
        "tailwindcss": "^3.3.1",
        "tslib": "^2.5.0",
